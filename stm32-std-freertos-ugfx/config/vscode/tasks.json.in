{
    "version": "2.0.0",
    "options": {
        "cwd": "@CMAKE_BINARY_DIR@"
    },
    "tasks": [
        {
            "label": "Clean",
            "type": "shell",
            "command": "make clean",
            "problemMatcher": []
        },
        {
            "label": "Build",
            "type": "shell",
            "command": "make all",
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": ["absolute"]
            }
        },
        {
            "label": "BuildFlash",
            "type": "shell",
            "command": "make flash",
            "problemMatcher": []
        },
        {
            "label": "Debug",
            "type": "shell",
            "command": "make debug",
            "problemMatcher": []
        }
    ]
}
